{
  "name": "goodeggs-formatters",
  "version": "1.6.5",
  "description": "formatters for good eggs",
  "author": "Good Eggs <open-source@goodeggs.com>",
  "contributors": [
    "Michael Kebbekus <michael.kebbekus@goodeggs.com>",
    "Michelle Becker <michelle.becker@goodeggs.com>"
  ],
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/goodeggs/goodeggs-formatters.git"
  },
  "homepage": "https://github.com/goodeggs/goodeggs-formatters",
  "bugs": "https://github.com/goodeggs/goodeggs-formatters/issues",
  "dependencies": {
    "accounting": "^0.4.1",
    "format-location": "^1.1.2",
    "fraction.js": "^1.3.0",
    "getto": "0.0.2",
    "inflect": "^0.5.0",
    "node-clock": "^0.1.1",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/node": "^7.20.7",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@goodeggs/toolkit": "^7.0.1",
    "@goodeggs/tsconfig": "^2.0.2",
    "@types/accounting": "^0.4.2",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/sinon": "^10.0.13",
    "@types/underscore": "^1.11.4",
    "chai": "^1.10.0",
    "mocha": "6.2.3",
    "sinon": "^1.12.2",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "yarn run build:clean && yarn run build:transpile && yarn run build:types",
    "build:clean": "rm -rf lib",
    "build:transpile": "babel --extensions=.ts,.js,.jsx,.tsx src --out-dir lib --copy-files",
    "build:types": "tsc --project tsconfig.declarations.json",
    "prepublish": "npm run build",
    "lint": "getk run lint-es '**/*.{js,jsx,ts,tsx}' --ignore-path .gitignore",
    "lint:fix": "getk run fix-es --ignore-path .gitignore",
    "test": "yarn run typecheck && yarn run lint && yarn run test:mocha",
    "test:mocha": "yarn run test:mocha:glob 'src/**/*.test.ts'",
    "test:mocha:glob": "NODE_ENV=test mocha --opts mocha.opts --extension ts,js,jsx,tsx",
    "typecheck": "tsc"
  },
  "publishConfig": {
    "registry": "https://npm.goodeggs.com/",
    "always-auth": true
  }
}
